{
  "name": "AnimeLogger",
  "version": "1.0",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["anime website"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
  "background": {
    "scripts": ["jquery-3.4.1.js", "background.js"],
    "persistent": false
  },
  "permissions": [
    "http://localhost/*",
    "storage"
  ]
}