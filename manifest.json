{
  "name": "AnimeLogger",
  "version": "1.0",
  "manifest_version": 2,
  "icons": {
    "128": "comfy.png"
  },
  "content_scripts": [
    {
      "matches": ["ANIME WEBSITE"],
      "js": ["scripts/content.js"],
      "run_at": "document_end"
    }
  ],
  "background": {
    "scripts": ["scripts/jquery-3.4.1.js", "scripts/background.js"],
    "persistent": false
  },
  "permissions": [
    "http://localhost/*",
    "tabs",
    "webNavigation",
    "*://*/*"
  ]
}